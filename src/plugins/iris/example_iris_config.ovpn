client
dev tun
proto indirect
remote host_address
persist-key
persist-tun
cipher AES-256-CBC
comp-lzo
verb 1
mute 20
route-method exe
route-delay 2
route 0.0.0.0 0.0.0.0
float
auth-retry interact
ifconfig-nowarn
plugin path_to_openvpn-plugin-iris.so_file password
ca path_to_ca.cert_file
